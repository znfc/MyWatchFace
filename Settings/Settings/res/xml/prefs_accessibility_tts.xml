<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/tts_settings_title"
    android:orderingFromXml="false">

    <!-- TTS Engine list will go over here, ordering will need to be set dynamically -->
    <ListPreference
        android:key="pref_accessibility_tts_rates"
        android:title="@string/tts_default_rate_title"
        android:icon="@drawable/ic_cc_settings_morehorizontal"
        android:entries="@array/tts_rate_entries"
        android:entryValues="@array/tts_rate_entry_values"
        android:persistent="true">
    </ListPreference>

</PreferenceScreen>
